<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE resource-adaptor-type-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Resource Adaptor Type 1.1//EN"
                                           "http://java.sun.com/dtd/slee-resource-adaptor-type-jar_1_1.dtd">
<resource-adaptor-type-jar>
	<resource-adaptor-type>
		<description>AMQP Resource Adaptor Type</description>
		<resource-adaptor-type-name>AMQPResourceAdaptorType</resource-adaptor-type-name>
		<resource-adaptor-type-vendor>org.mobicents</resource-adaptor-type-vendor>
		<resource-adaptor-type-version>1.0</resource-adaptor-type-version>
		
		<library-ref>
			<library-name>library-amqp</library-name>
			<library-vendor>library-amqp</library-vendor>
			<library-version>1.0</library-version>
		</library-ref>
		
		<resource-adaptor-type-classes>
			<activity-type>
				<activity-type-name>
					org.mobicents.slee.resource.amqp.AMQPActivity
				</activity-type-name>
			</activity-type>
			<activity-context-interface-factory-interface>
				<activity-context-interface-factory-interface-name>
					org.mobicents.slee.resource.amqp.AMQPActivityContextInterfaceFactory
				</activity-context-interface-factory-interface-name>
			</activity-context-interface-factory-interface>
			<resource-adaptor-interface>
				<resource-adaptor-interface-name>
					org.mobicents.slee.resource.amqp.AMQPResourceAdaptorSbbInterface
				</resource-adaptor-interface-name>
			</resource-adaptor-interface>
			
		</resource-adaptor-type-classes>
		
		<event-type-ref>
			<event-type-name>AMQPEvent</event-type-name>
			<event-type-vendor>org.mobicents</event-type-vendor>
			<event-type-version>1.0</event-type-version>
		</event-type-ref>
	</resource-adaptor-type>
</resource-adaptor-type-jar>
