<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE resource-adaptor-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Resource Adaptor 1.1//EN"
                                      "http://java.sun.com/dtd/slee-resource-adaptor-jar_1_1.dtd">
<resource-adaptor-jar>
    <resource-adaptor>
        <description>AMQP Resource Adaptor 1.0</description>
        <resource-adaptor-name>AMQPResourceAdaptor</resource-adaptor-name>
        <resource-adaptor-vendor>org.mobicents</resource-adaptor-vendor>
        <resource-adaptor-version>1.0</resource-adaptor-version>
        <resource-adaptor-type-ref>
            <resource-adaptor-type-name>AMQPResourceAdaptorType</resource-adaptor-type-name>
            <resource-adaptor-type-vendor>org.mobicents</resource-adaptor-type-vendor>
            <resource-adaptor-type-version>1.0</resource-adaptor-type-version>
        </resource-adaptor-type-ref>
        <resource-adaptor-classes>
            <resource-adaptor-class supports-active-reconfiguration="True">
                <resource-adaptor-class-name>org.mobicents.slee.resource.amqp.AMQPResourceAdaptor</resource-adaptor-class-name>
            </resource-adaptor-class>
        </resource-adaptor-classes>
        <config-property>
            <config-property-name>amqpHost</config-property-name>
            <config-property-type>java.lang.String</config-property-type>
            <config-property-value>amqp-host</config-property-value>
        </config-property>
        <config-property>
            <config-property-name>amqpPort</config-property-name>
            <config-property-type>java.lang.Integer</config-property-type>
            <config-property-value>5672</config-property-value>
        </config-property>
        <config-property>
            <config-property-name>username</config-property-name>
            <config-property-type>java.lang.String</config-property-type>
            <config-property-value>guest</config-property-value>
        </config-property>
        <config-property>
            <config-property-name>password</config-property-name>
            <config-property-type>java.lang.String</config-property-type>
            <config-property-value>guest</config-property-value>
        </config-property>
        <config-property>
            <config-property-name>amqpQeueName</config-property-name>
            <config-property-type>java.lang.String</config-property-type>
            <config-property-value>myQueue</config-property-value>
        </config-property>
        <config-property>
            <config-property-name>amqpExchangeName</config-property-name>
            <config-property-type>java.lang.String</config-property-type>
            <config-property-value>myExchange</config-property-value>
        </config-property>
    </resource-adaptor>
</resource-adaptor-jar>
